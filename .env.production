# 生产环境服务器暂时未配，上线时根据服务器配置进行调整

# Node.js 运行环境
NODE_ENV = production

# 应用运行环境标识
VITE_APP_ENV = production

# 生产 API 地址 (正式后端域名)
VITE_API_BASE_URL = https://manpaodomain.com

# 生产 WebSocket 地址 (注意使用 wss 协议)
VITE_WS_BASE_URL = wss://manpaodomain.com

# 生产 SSE 地址
VITE_SSE_BASE_URL = https://manpaodomain.com

# 生产 CDN 地址，用于加载静态资源
VITE_CDN_PREFIX = https://cdn.manpaodomain.com

# 是否使用mock
VITE_USE_MOCK = false